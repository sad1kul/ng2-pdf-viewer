{
  "name": "@sad1kul/ng2-pdf-viewer",
  "version": "1.0.17",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "description": "Angular 17 component for rendering PDF",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/ng2-pdf-viewer/stats.json",
    "test": "ng test --watch false",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:w": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "ng build --configuration production --output-path docs --base-href ng2-pdf-viewer && cp ./docs/index.html ./docs/404.html"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sad1kul/ng2-pdf-viewer.git"
  },
  "keywords": [
    "angular",
    "pdf",
    "angular",
    "angular-pdf",
    "angular2-pdf",
    "ng2",
    "ng2-pdf",
    "angular5"
  ],
  "author": "Sadikul Islam <sadik@sadikul.me>",
  "bugs": {
    "url": "https://github.com/sad1kul/ng2-pdf-viewer/issues"
  },
  "homepage": "https://github.com/sad1kul/ng2-pdf-viewer#readme",
  "dependencies": {
    "@sad1kul/ng2-pdf-viewer": "file:",
    "pdfjs-dist": "4.8.69",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular/animations": "^17.3.12",
    "@angular/cli": "~17.3.11",
    "@angular/compiler": "^17.3.12",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/language-service": "17.3.12",
    "@angular/material": "^16.0.4",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.3.1",
    "@types/pdfjs-dist": "^2.10.377",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "postcss": "^8.5.1",
    "postcss-purgecss": "^5.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^5.5.0",
    "rxjs": "~7.8.0",
    "typescript": "~5.3.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "zone.js": "~0.14.10"
  }
}
